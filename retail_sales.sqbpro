    c.CustomerName,
    p.ProductName,
    t.Date,
    s.StoreName,
    f.SalesAmount
FROM FactSales f
JOIN DimCustomer c ON f.CustomerID = c.CustomerID
JOIN DimProduct p ON f.ProductID = p.ProductID
JOIN DimTime t ON f.TimeID = t.TimeID
JOIN DimStore s ON f.StoreID = s.StoreID;





